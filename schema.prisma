datasource db {
  provider = "postgresql"
  url = "postgresql://postgres:password@localhost:5432/ap_db"
}
generator client {
  provider = "prisma-client-js"
}
model Color {
  id    Int     @id @default(autoincrement())
  name  String
}
seed {
  // Define the name of the seed file
  migration1 {
    // Define the data to be seeded
    create: [
      { name: "Red" },
      { name: "Blue" },
      { name: "Green" }
    ]
  }
}
